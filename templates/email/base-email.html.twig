    <link rel="mask-icon" href="{{ absolute_url(asset('/dist/images/favicon/safari-pinned-tab.svg')) }}" color="#267d8a">
    <meta name="apple-mobile-web-app-title" content="Clover">
    <meta name="application-name" content="Clover">
    <meta name="msapplication-TileColor" content="#00aba9">
    <meta name="theme-color" content="#ffffff">

    <title>{% block title %}Email Title{% endblock %}</title>

    <!-- ligne de 1000 espace http://www.emailonacid.com/blog/details/C13/ensure_that_your_entire_email_is_rendered_by_default_in_the_iphone_ipad -->

